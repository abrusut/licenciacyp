{% extends "MProdLicenciaCyPBundle:Page:index.html.twig" %}

{% block nuevoybarradebusquedas %}
{% endblock %}
{% block styles %}
     <!-- Le styles -->
        <link href="{{ asset('bundles/petkoparacrudgenerator/css/bootstrap.min.css') }}" rel="stylesheet">    
        <link href="{{ asset('css/ajustesPetkoparaStg.css') }}" rel="stylesheet">    
{% endblock %}

{% block javascriptsIncludes %} 
        <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
        <!--[if lt IE 9]>
        <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->
    <script src="{{ asset('bundles/petkoparacrudgenerator/js/jquery.min.js') }}"></script>
    <script src="{{ asset('bundles/petkoparacrudgenerator/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('bundles/petkoparacrudgenerator/js/petkopara-crud-generator.js') }}"></script>
    
{% endblock %} 

{% block title %}
Rendicion
{% endblock %}

{% block cuerpo %}
{% block body %}

<div  class="row">
    <div class="page-header">
        <h2><a href="{{ path('rendicion') }}">Ver Rendicion <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span></a> </h2>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <p><strong>Id</strong></p>
        <p>
            {{ rendicion.id }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Archivo</strong></p>
        <p>
            {{ rendicion.fileRendicionLiquidacion.nombreOriginalArchivo }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Numero Rendicion</strong></p>
        <p>
            {{ rendicion.numeroRendicion }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Nis</strong></p>
        <p>
            {{ rendicion.nis }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Fecha Cobro</strong></p>
        <p>
            {% if rendicion.fechaCobro %}{{ rendicion.fechaCobro|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Codigo Barra Cliente</strong></p>
        <p>
            {{ rendicion.codigoBarraCliente }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Importe Cobrado</strong></p>
        <p>
            {{ rendicion.importeCobrado }}
        </p>

    </div>
  
    <div class="col-md-6">
        <p><strong>Fecha Alta</strong></p>
        <p>
            {% if rendicion.createdAt %}{{ rendicion.createdAt|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div></div>

<hr/>


<div class="form-group">
    <a class="btn btn-default" href="{{ path('rendicion') }}">
        <span class="glyphicon glyphicon-list" aria-hidden="true"></span>
        Volver al Listado
    </a>
    <a class="btn btn-primary" href="{{ path('rendicion_edit', { 'id':  rendicion.id }) }}">
        <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        Editar
    </a>
   
</div>

</div>
{% endblock %}
{% endblock %}

