{% extends "MProdLicenciaCyPBundle:Page:index.html.twig" %}

{% block nuevoybarradebusquedas %}
{% endblock %}
{% block styles %}
     <!-- Le styles -->
        <link href="{{ asset('bundles/petkoparacrudgenerator/css/bootstrap.min.css') }}" rel="stylesheet">    
        <link href="{{ asset('css/ajustesPetkoparaStg.css') }}" rel="stylesheet">    
{% endblock %}

{% block javascriptsIncludes %} 
        <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
        <!--[if lt IE 9]>
        <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->
    <script src="{{ asset('bundles/petkoparacrudgenerator/js/jquery.min.js') }}"></script>
    <script src="{{ asset('bundles/petkoparacrudgenerator/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('bundles/petkoparacrudgenerator/js/petkopara-crud-generator.js') }}"></script>
     
{% endblock %} 

{% block title %}
Liquidacion
{% endblock %}

{% block cuerpo %}
{% block body %}

<div class="row">
    <div class="page-header">
        <h2><a href="{{ path('liquidacion') }}">Ver Liquidacion <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span></a> </h2>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <p><strong>Id</strong></p>
        <p>
            {{ liquidacion.id }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Nis</strong></p>
        <p>
            {{ liquidacion.nis }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Fecha Cobro</strong></p>
        <p>
            {% if liquidacion.fechaCobro %}{{ liquidacion.fechaCobro|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Codigo Barra Cliente</strong></p>
        <p>
            {{ liquidacion.codigoBarraCliente }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Numero Rendicion</strong></p>
        <p>
            {{ liquidacion.numeroRendicion }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Importe Cobrado</strong></p>
        <p>
            {{ liquidacion.importeCobrado }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Comision</strong></p>
        <p>
            {{ liquidacion.comision }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Fecha Lectura Archivo</strong></p>
        <p>
            {% if liquidacion.fechaLecturaArchivo %}{{ liquidacion.fechaLecturaArchivo|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Fecha Creacion Archivo</strong></p>
        <p>
            {% if liquidacion.fechaCreacionArchivo %}{{ liquidacion.fechaCreacionArchivo|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Fecha Alta</strong></p>
        <p>
            {% if liquidacion.createdAt %}{{ liquidacion.createdAt|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div></div>

<hr/>


<div class="form-group">
    <a class="btn btn-default" href="{{ path('liquidacion') }}">
        <span class="glyphicon glyphicon-list" aria-hidden="true"></span>
        Volver al Listado
    </a>
    <a class="btn btn-primary" href="{{ path('liquidacion_edit', { 'id':  liquidacion.id }) }}">
        <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        Editar
    </a>
    
</div>

</div>
{% endblock %}
{% endblock %}

