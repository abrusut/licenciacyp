{% extends "MProdLicenciaCyPBundle:Page:index.html.twig" %} {% block
nuevoybarradebusquedas %} {% endblock %} {% block styles %}
<!-- Le styles -->
<link
  href="{{ asset('bundles/petkoparacrudgenerator/css/bootstrap.min.css') }}"
  rel="stylesheet"
/>
<link href="{{ asset('css/ajustesPetkoparaStg.css') }}" rel="stylesheet" />
{% endblock %} {% block javascriptsIncludes %}
<!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
<!--[if lt IE 9]>
  <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
<![endif]-->
<script src="{{
    asset('bundles/petkoparacrudgenerator/js/jquery.min.js')
  }}"></script>
<script src="{{
    asset('bundles/petkoparacrudgenerator/js/bootstrap.min.js')
  }}"></script>
<script src="{{
    asset('bundles/petkoparacrudgenerator/js/petkopara-crud-generator.js')
  }}"></script>

{% endblock %} {% block title %} Tipo Licencia {% endblock %} 

{% block cuerpo %}
{% block body %}


  <div class="col-md-12">
    <div class="page-header">
      <h2>
        <a href="{{ path('tipolicencia') }}"
          >Nuevo Tipo Licencia
          <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span
        ></a>
      </h2>
    </div>
  </div>

  
      <div class="col-md-6">
        <p><strong>Id</strong></p>
        <p>
          {{ tipoLicencium.id }}
        </p>
      </div>
      <div class="col-md-6">
        <p><strong>Descripcion</strong></p>
        <p>
          {{ tipoLicencium.descripcion }}
        </p>
      </div>
      <div class="col-md-6">
        <p><strong>Genero/ Jubilado</strong></p>
        <p>
          {{ tipoLicencium.generoJubilado }}
        </p>
      </div>
      <div class="col-md-6">
        <p><strong>Cliente</strong></p>
        <p>
          {{ tipoLicencium.clienteSap }}
        </p>
      </div>
      <div class="col-md-6">
        <p><strong>Letra Servicio</strong></p>
        <p>
          {{ tipoLicencium.letraServicio }}
        </p>
      </div>
      <div class="col-md-6">
        <p><strong>Arancel</strong></p>
        <p>
          {{ tipoLicencium.arancel }}
        </p>
      </div>
     
      <div class="col-md-6">
        <p><strong>Dias Vigencia</strong></p>
        <p>
          {{ tipoLicencium.diasVigencia }}
        </p>
      </div>
     
    <div class="col-md-6">
      <p><strong>Fecha Tope</strong></p>
      <p>
        {% if tipoLicencium.fechaTope %}{{ tipoLicencium.fechaTope|date('d-m-Y H:i:s')

        }}{% endif %}
      </p>
    </div>
    <div class="col-md-6">
      <p><strong>Activo</strong></p>
      <p>
        {% if tipoLicencium.isActive %}Yes{% else %}No{% endif %}
      </p>
    </div>
    <div class="col-md-6">
      <p><strong>Aplica en Provincia</strong></p>
      <p>
        {%- if tipoLicencium.aplicaEnProvincia == "SF" or
        tipoLicencium.aplicaEnProvincia == "sf" %}Santa Fe{% else %}{{
          tipoLicencium.aplicaEnProvincia
        }}{% endif %}
      </p>
    </div>

    <hr />

    <div class="row col-md-12">
      <a class="btn btn-default" href="{{ path('tipolicencia') }}">
        <span class="glyphicon glyphicon-list" aria-hidden="true"></span>
        Volver al Listado
      </a>
      <a
        class="btn btn-primary"
        href="{{ path('tipolicencia_edit', { id: tipoLicencium.id }) }}"
      >
        <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        Editar
      </a>
    </div>
  </div>
    {% endblock %} {% endblock %}
  

