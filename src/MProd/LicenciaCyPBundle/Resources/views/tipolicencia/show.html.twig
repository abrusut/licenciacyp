{% extends "MProdLicenciaCyPBundle:Page:index.html.twig" %}

{% block nuevoybarradebusquedas %}
{% endblock %}
{% block styles %}
     <!-- Le styles -->
        <link href="{{ asset('bundles/petkoparacrudgenerator/css/bootstrap.min.css') }}" rel="stylesheet">               
{% endblock %}

{% block javascriptsIncludes %} 
        <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
        <!--[if lt IE 9]>
        <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->
    <script src="{{ asset('bundles/petkoparacrudgenerator/js/jquery.min.js') }}"></script>
    <script src="{{ asset('bundles/petkoparacrudgenerator/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('bundles/petkoparacrudgenerator/js/petkopara-crud-generator.js') }}"></script>
     
{% endblock %} 

{% block title %}
Tipo Licencia
{% endblock %}

{% block cuerpo %}
{% block body %}

<div id="top" class="row">
    <div class="page-header">
        <h2>View tipoLicencium <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span> </h2>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <p><strong>Id</strong></p>
        <p>
            {{ tipoLicencium.id }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Descripcion</strong></p>
        <p>
            {{ tipoLicencium.descripcion }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Generojubilado</strong></p>
        <p>
            {{ tipoLicencium.generoJubilado }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Clientesap</strong></p>
        <p>
            {{ tipoLicencium.clienteSap }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Letraservicio</strong></p>
        <p>
            {{ tipoLicencium.letraServicio }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Arancel</strong></p>
        <p>
            {{ tipoLicencium.arancel }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Porcentajerecargoprimervencimiento</strong></p>
        <p>
            {{ tipoLicencium.porcentajeRecargoPrimerVencimiento }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Porcentajerecargosegundovencimiento</strong></p>
        <p>
            {{ tipoLicencium.porcentajeRecargoSegundoVencimiento }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Diasvigencia</strong></p>
        <p>
            {{ tipoLicencium.diasVigencia }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Diasprimervencimiento</strong></p>
        <p>
            {{ tipoLicencium.diasPrimerVencimiento }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Diassegundovencimiento</strong></p>
        <p>
            {{ tipoLicencium.diasSegundoVencimiento }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Fechatope</strong></p>
        <p>
            {% if tipoLicencium.fechaTope %}{{ tipoLicencium.fechaTope|date('Y-m-d H:i:s') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Isactive</strong></p>
        <p>
            {% if tipoLicencium.isActive %}Yes{% else %}No{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Aplicaenprovincia</strong></p>
        <p>
            {{ tipoLicencium.aplicaEnProvincia }}
        </p>

    </div></div>

<hr/>


<div class="form-group">
    <a class="btn btn-default" href="{{ path('tipolicencia') }}">
        <span class="glyphicon glyphicon-list" aria-hidden="true"></span>
        Back to list
    </a>
    <a class="btn btn-primary" href="{{ path('tipolicencia_edit', { 'id':  tipoLicencium.id }) }}">
        <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        Edit
    </a>
    <a class="btn btn-info" href="{{ path('tipolicencia_new') }}">
        <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>
        New
    </a>
    <form action="{{ path('tipolicencia_delete', { 'id': tipoLicencium.id }) }}" method="post" style="display: inline-block">
        <input type="hidden" name="_method" value="DELETE" />
        {{ form_widget(delete_form) }}
        <button class="btn btn-danger" type="submit" onclick="return confirm('Are you sure');">
            <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
            Delete
        </button>
    </form>
</div>


{% endblock %}

{% endblock %} 