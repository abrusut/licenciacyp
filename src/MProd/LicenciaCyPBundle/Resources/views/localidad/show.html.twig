{% endblock %} 

{% block title %}
 localidad index
{% endblock %}

{% block cuerpo %}
{% block body %}
   
<div class="row">
    <div class="page-header">    
        <h2><a href="{{ path('localidad') }}">Ver localidad 
        <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span> </a></h2>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <p><strong>Id</strong></p>
        <p>
            {{ localidad.id }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Distrito</strong></p>
        <p>
            {{ localidad.ldistrito }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Nombre Distrito</strong></p>
        <p>
            {{ localidad.lnomdis }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Departamento</strong></p>
        <p>
            {{ localidad.ldepartamento }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Nombre Departamento</strong></p>
        <p>
            {{ localidad.lnomdpto }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Nodo</strong></p>
        <p>
            {{ localidad.nodo }}
        </p>

    </div></div>

<hr/>


<div class="form-group">
    <a class="btn btn-default" href="{{ path('localidad') }}">
        <span class="glyphicon glyphicon-list" aria-hidden="true"></span>
        Volver al Listado
    </a>
    <a class="btn btn-primary" href="{{ path('localidad_edit', { 'id':  localidad.id }) }}">
        <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        Editar
    </a>
   
</div>
</div>


{% endblock %}

