{% extends "MProdLicenciaCyPBundle:Page:index.html.twig" %}

{% block nuevoybarradebusquedas %}
{% endblock %}
{% block styles %}
     <!-- Le styles -->
        <link href="{{ asset('bundles/petkoparacrudgenerator/css/bootstrap.min.css') }}" rel="stylesheet">               
         <link href="{{ asset('css/ajustesPetkoparaStg.css') }}" rel="stylesheet">
{% endblock %}

{% block javascriptsIncludes %} 
        <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
        <!--[if lt IE 9]>
        <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->
    <script src="{{ asset('bundles/petkoparacrudgenerator/js/jquery.min.js') }}"></script>
    <script src="{{ asset('bundles/petkoparacrudgenerator/js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('bundles/petkoparacrudgenerator/js/petkopara-crud-generator.js') }}"></script>
      
{% endblock %} 

{% block title %}
{{ parent() }} - usuario 
{% endblock %}

{% block cuerpo %}
{% block body %}

<div class="row">
    <div class="page-header">
        <h2><a href="{{ path('usuario') }}">Ver usuario 
        <span class="glyphicon glyphicon-eye-open" aria-hidden="true"></span></a> </h2>
    </div>
</div>

<div class="row">
    <div class="col-md-6">
        <p><strong>Id</strong></p>
        <p>
            {{ usuario.id }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Nombre</strong></p>
        <p>
            {{ usuario.nombre }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Apellido</strong></p>
        <p>
            {{ usuario.apellido }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Dni</strong></p>
        <p>
            {{ usuario.dni }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Telefono</strong></p>
        <p>
            {{ usuario.telefono }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Username</strong></p>
        <p>
            {{ usuario.username }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Usernamecanonical</strong></p>
        <p>
            {{ usuario.usernameCanonical }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Email</strong></p>
        <p>
            {{ usuario.email }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Emailcanonical</strong></p>
        <p>
            {{ usuario.emailCanonical }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Enabled</strong></p>
        <p>
            {% if usuario.enabled %}Yes{% else %}No{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Salt</strong></p>
        <p>
            {{ usuario.salt }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Password</strong></p>
        <p>
            {{ usuario.password }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Lastlogin</strong></p>
        <p>
            {% if usuario.lastLogin %}{{ usuario.lastLogin|date('Y-m-d') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Confirmationtoken</strong></p>
        <p>
            {{ usuario.confirmationToken }}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Passwordrequestedat</strong></p>
        <p>
            {% if usuario.passwordRequestedAt %}{{ usuario.passwordRequestedAt|date('Y-m-d') }}{% endif %}
        </p>

    </div>
    <div class="col-md-6">
        <p><strong>Roles</strong></p>
        <p>
            {% if usuario.roles %}{{ usuario.roles|join(', ') }}{% endif %}
        </p>

    </div></div>

<hr/>


<div class="form-group">
    <a class="btn btn-default" href="{{ path('usuario') }}">
        <span class="glyphicon glyphicon-list" aria-hidden="true"></span>
        Back to list
    </a>
    <a class="btn btn-primary" href="{{ path('usuario_edit', { 'id':  usuario.id }) }}">
        <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
        Edit
    </a>
    <a class="btn btn-info" href="{{ path('usuario_new') }}">
        <span class="glyphicon glyphicon-plus" aria-hidden="true"></span>
        New
    </a>
    <form action="{{ path('usuario_delete', { 'id': usuario.id }) }}" method="post" style="display: inline-block">
        <input type="hidden" name="_method" value="DELETE" />
        {{ form_widget(delete_form) }}
        <button class="btn btn-danger" type="submit" onclick="return confirm('Are you sure');">
            <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
            Delete
        </button>
    </form>
</div>
</div>
{% endblock %}
{% endblock %}

