{% extends 'ThemeAplicativoBundle:Default:base.html.twig'   %} 

{% block top %}

    <div class="row">
        <div class="twelvecol">
            <div class="contents n5">            
                <h2 class="titulo_aplicativo">Componentes de Assets</h2>
                <p class="organismo">{{ encabezado_nombre_dependencia | default('Nombre de dependencia') }}. {{ encabezado_nombre_organismo | default('Nombre de organismo') }}</p>                
            </div>
        </div>
    </div>   


{% endblock %}

{% block barraHerramientas %}                

{% endblock %}
{% block menu %} 
    {% block submenus %}
    {% endblock %}
{% endblock %}



{% block cuerpo %}    

    <div class="row form">
        <form>
            <h3>Componente:</h3>
            <div class="row">
                <div class="twelvecol">                    
                    <div class="ui-widget">
                        <div style="padding: 10px;">
                            <p>
                                <span style="float: left; margin-right: .3em;" class="ui-icon ui-icon-info"></span> <strong>Elementos:</strong>
                                {{ componente | raw }}
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </form>
    </div>

{% endblock %} 
